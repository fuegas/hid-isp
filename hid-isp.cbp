<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="hid-isp-Firmware" />
		<Option pch_mode="2" />
		<Option compiler="avrgcc" />
		<Build>
			<Target title="Release">
				<Option output="hid-isp-firmware" prefix_auto="1" extension_auto="1" />
				<Option object_output=".obj\Release\" />
				<Option type="1" />
				<Option compiler="avrgcc" />
				<Compiler>
					<Add option="-mmcu=atmega8" />
					<Add option="-Os" />
					<Add option="-DF_CPU=20000000" />
					<Add directory="..\firmware" />
					<Add directory="usbdrv" />
				</Compiler>
				<ExtraCommands>
					<Add after="avr-objcopy -j .text -j .data -O ihex hid-isp-firmware.exe hid-isp-firmware.hex" />
					<Add after="avr-size -C hid-isp-firmware.exe" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="src/main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="src/requests.h" />
		<Unit filename="src/usbconfig.h" />
		<Unit filename="src/usb/usbdrv.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="src/usb/usbdrv.h" />
		<Unit filename="src/usb/usbdrvasm.S" />
		<Unit filename="src/usb/usbportability.h" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
